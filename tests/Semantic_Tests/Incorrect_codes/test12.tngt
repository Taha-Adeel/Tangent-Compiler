#example to demonstrate inbuilt datatypes

driver<: :>
(:
	var Circle my_circle;
	my_circle>>set_radius(2.3);
	my_circle>>set_colour(get_colour_from_rgb(20,122,0));
	my_circle>>set_center(make_point(200,200));

	var Image my_image;
	my_image>>set_dimension(300,300);
	my_image>>draw(my_circle);
	my_image>>output("outputfile.svg");
:)
